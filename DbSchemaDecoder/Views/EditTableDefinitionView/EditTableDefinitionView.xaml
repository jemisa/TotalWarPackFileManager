 <UserControl x:Class="DbSchemaDecoder.EditTableDefinitionView.ConfigureTableRowsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DbSchemaDecoder.EditTableDefinitionView" xmlns:edittabledefinitionview="clr-namespace:DbSchemaDecoder.Views.EditTableDefinitionView"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    
    <Grid>
        <TabControl>
            <TabItem Header="CA Schema Explorer">
                <Grid Background="#FFE5E5E5">
                    <local:CreativeAssemblySchemaExplorer/>
                </Grid>
            </TabItem>
            <TabItem Header="Field Explorer">
                <Grid >
                    <local:DecodeDbTypeView VerticalAlignment="Top"/>
                </Grid>
            </TabItem>
            <TabItem Header="Brute force">
                <Grid >
                    <edittabledefinitionview:BruteForceView></edittabledefinitionview:BruteForceView>
                </Grid>
            </TabItem>
        </TabControl>

    </Grid>
</UserControl>
