<UserControl x:Class="DbSchemaDecoder.DisplayTableDefinitionView.DbTableView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DbSchemaDecoder.DisplayTableDefinitionView"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <ListView>
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Name" Width="100"/>
                    <GridViewColumn Header="Value" Width="100"/>
                    <GridViewColumn Header="IsKey" Width="100"/>
                    <GridViewColumn Header="Ref table" Width="100"/>
                </GridView>
            </ListView.View>
        </ListView>

    </Grid>
</UserControl>
