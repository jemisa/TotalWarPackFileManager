<UserControl x:Class="DbSchemaDecoder.DisplayTableDefinitionView.DisplayTableDefinitionView2"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DbSchemaDecoder.DisplayTableDefinitionView"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TabControl>
            <TabItem Header="Table definitions">
                <Grid Background="#FFE5E5E5">
                    <local:TableDefinitionView></local:TableDefinitionView>
                </Grid>
            </TabItem>
            <TabItem x:Name="DbTableViewTabItem"  Header="Enties">
                <local:DbTableView></local:DbTableView>
            </TabItem>
        </TabControl>

    </Grid>
</UserControl>
